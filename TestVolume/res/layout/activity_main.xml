<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF000FF0" xmlns:app1="http://schemas.android.com/apk/res/com.example.testvolume">

    <com.wzx.testvolume.NumTipSeekBar
        android:id="@+id/ntsb_seekbar1"
        android:layout_width="500dp"
        android:layout_height="40dp"
        android:layout_marginTop="50dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        app:circleButtonColor="@color/white"
        app:circleButtonRadius="16dp"
        app:circleButtonTextColor="@color/purple_82"
        app:circleButtonTextSize="16sp"
        app:maxProgress="10"
        app:progressColor="@color/white"
        app:progressHeight="2dp"
        app:selectProgress="1"
        app:tickBarColor="@color/white_aa"
        app:tickBarHeight="2dp"    
        
        
        />

    <com.wzx.testvolume.NumSeekBar
        android:id="@+id/ntsb_seekbar"
        android:layout_width="500dp"
        android:layout_height="40dp"
        android:layout_marginTop="200dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:visibility="invisible"
        
 />
</RelativeLayout>